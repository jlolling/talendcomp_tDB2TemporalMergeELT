<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="3.0" STATUS="PROD"
		COMPATIBILITY="ALL" AUTHOR="cimt AG" RELEASE_DATE="20141001A"
		STARTABLE="true" SCHEMA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/DB2</FAMILY>
		<FAMILY>Business_Intelligence/DB_SCD</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="0"
			MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" GROUP="CONNECTION_CONFIG"
			NUM_ROW="10" REPOSITORY_VALUE="DATABASE:IBM_DB2" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="11" GROUP="CONNECTION_CONFIG">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="tDB2Connection" NUM_ROW="11" SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			DYNAMIC_SETTINGS="true" GROUP="CONNECTION_CONFIG" />

		<PARAMETER NAME="HOST" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SERVER_NAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" GROUP="CONNECTION_CONFIG" FIELD="TEXT"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PORT">
			<DEFAULT>"50000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION_PROPERTIES" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="31" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" REQUIRED="false">
			<DEFAULT>"retrieveMessagesFromServerOnGetMessage=true"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="32" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SID" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_SCHEMA" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="32" REPOSITORY_VALUE="SCHEMA" REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="40" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="40" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PASSWORD" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" GROUP="SOURCE_CONFIG"
			NUM_ROW="50" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="WHERE" FIELD="MEMO" GROUP="SOURCE_CONFIG"
			NUM_ROW="51" >
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" GROUP="SOURCE_CONFIG"
			NUM_ROW="52">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_SELF_DEFINED_SOURCE_KEY" GROUP="SOURCE_CONFIG" FIELD="CHECK"
			NUM_ROW="54" REQUIRED="false">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SUPPLEMENTARY_SOURCE_COLUMN_CONFIG" FIELD="TABLE"
			NUM_ROW="55" NB_LINES="10" GROUP="SOURCE_CONFIG" SHOW_IF="USE_SELF_DEFINED_SOURCE_KEY == 'true'">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="IS_SOURCE_KEY" FIELD="CHECK" VALUE="false" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="TARGET_SCHEMA" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLE" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BUSINESS_TIMEPERIOD_IS_DATE" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="65">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BUSINESS_TIMEPERIOD_START_VALUE" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="66" REQUIRED="true">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="BUSINESS_TIMEPERIOD_END_VALUE" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="68" REQUIRED="true">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SWITCH_ON_VERSIONING_AT_START" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="80">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SWITCH_OFF_VERSIONING_AT_THE_END" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="80" SHOW_IF="SWITCH_ON_VERSIONING_AT_START == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="NO_EXEC_MODE" FIELD="TEXT" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEBUG" FIELD="CHECK" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_STATS" FIELD="TEXT" NUM_ROW="30">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_STATS_FILE" FIELD="FILE" NUM_ROW="40">
			<DEFAULT></DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cimt.talendcomp.db2scdelt" MODULE="cimt.talendcomp.db2scdelt-3.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="Driver-DB2" MODULE="db2jcc.jar"
				UrlPath="platform:/plugin/org.talend.libraries.jdbc.db2/lib/db2jcc.jar"
				REQUIRED="true" />
			<IMPORT NAME="Driver-DB2" MODULE="db2jcc_license_cu.jar"
				UrlPath="platform:/plugin/org.talend.libraries.jdbc.db2/lib/db2jcc_license_cu.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_DELETED" TYPE="id_Integer" />
	</RETURNS>
	
</COMPONENT>