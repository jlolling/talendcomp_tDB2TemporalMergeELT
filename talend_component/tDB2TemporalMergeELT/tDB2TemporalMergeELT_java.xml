<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="2.1" STATUS="PROD"
		COMPATIBILITY="ALL" AUTHOR="cimt AG" RELEASE_DATE="20140909A"
		STARTABLE="true" SCHEMA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/DB2</FAMILY>
		<FAMILY>Business_Intelligence/DB_SCD</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="0"
			MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" GROUP="CONNECTION_CONFIG"
			NUM_ROW="10" REPOSITORY_VALUE="DATABASE:IBM_DB2" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="11" GROUP="CONNECTION_CONFIG">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="tDB2Connection" NUM_ROW="11" SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			DYNAMIC_SETTINGS="true" GROUP="CONNECTION_CONFIG" />

		<PARAMETER NAME="HOST" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SERVER_NAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" GROUP="CONNECTION_CONFIG" FIELD="TEXT"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PORT">
			<DEFAULT>"50000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION_PROPERTIES" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="31" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" REQUIRED="false">
			<DEFAULT>"retrieveMessagesFromServerOnGetMessage=true"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="32" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SID" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_SCHEMA" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="32" REPOSITORY_VALUE="SCHEMA" REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="40" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="40" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PASSWORD" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" GROUP="SOURCE_CONFIG"
			NUM_ROW="50" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="WHERE" FIELD="MEMO" GROUP="SOURCE_CONFIG"
			NUM_ROW="51" >
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" GROUP="SOURCE_CONFIG"
			NUM_ROW="52">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_SELF_DEFINED_SOURCE_KEY" GROUP="SOURCE_CONFIG" FIELD="CHECK"
			NUM_ROW="54" REQUIRED="false">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TRACK_CHANGES_FOR_ALL_FIELD" GROUP="SOURCE_CONFIG" FIELD="CHECK"
			NUM_ROW="54" REQUIRED="false">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SUPPLEMENTARY_SOURCE_COLUMN_CONFIG" FIELD="TABLE"
			NUM_ROW="55" NB_LINES="10" GROUP="SOURCE_CONFIG">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="IGNORE" FIELD="CHECK" VALUE="false"/>
				<ITEM NAME="TRACK_CHANGES" FIELD="CHECK" VALUE="true" SHOW_IF="TRACK_CHANGES_FOR_ALL_FIELD == 'false'"/>
				<ITEM NAME="IS_SOURCE_KEY" FIELD="CHECK" VALUE="false" SHOW_IF="USE_SELF_DEFINED_SOURCE_KEY == 'true'"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="TARGET_SCHEMA" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLE" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLE_HIST" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="false">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE_TABLE_IF_NOT_EXISTS" FIELD="CHECK" GROUP="TARGET_CONFIG" NUM_ROW="62" />

		<PARAMETER NAME="CREATE_INDEXES" FIELD="CHECK" GROUP="TARGET_CONFIG" NUM_ROW="62" SHOW_IF="CREATE_TABLE_IF_NOT_EXISTS == 'true'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE_PK" FIELD="CHECK" GROUP="TARGET_CONFIG" NUM_ROW="62" SHOW_IF="(CREATE_TABLE_IF_NOT_EXISTS == 'true') and (USE_BUSINESS_TIMEPERIOD == 'true')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_SPECIFIC_TABLESPACE" GROUP="TARGET_CONFIG" FIELD="CHECK"
			NUM_ROW="63" SHOW_IF="CREATE_TABLE_IF_NOT_EXISTS == 'true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLESPACE" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="63" REQUIRED="true" SHOW_IF="(CREATE_TABLE_IF_NOT_EXISTS == 'true') and (USE_SPECIFIC_TABLESPACE == 'true')">
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SURROGATE_KEY" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="64" >
			<DEFAULT>"SKEY"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BUSINESS_TIMEPERIOD" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="65" REQUIRED="true">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BUSINESS_TIMEPERIOD_IS_DATE" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="65" SHOW_IF="USE_BUSINESS_TIMEPERIOD == 'true'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BUSINESS_TIMEPERIOD_START" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="66" REQUIRED="true" SHOW_IF="USE_BUSINESS_TIMEPERIOD == 'true'">
			<DEFAULT>"BUS_VALID_FROM"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="BUSINESS_TIMEPERIOD_START_VALUE" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="66" REQUIRED="true" SHOW_IF="USE_BUSINESS_TIMEPERIOD == 'true'">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="BUSINESS_TIMEPERIOD_END" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="67" REQUIRED="true" SHOW_IF="USE_BUSINESS_TIMEPERIOD == 'true'">
			<DEFAULT>"BUS_VALID_TO"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="BUSINESS_TIMEPERIOD_END_VALUE_FOR_INSERT" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="68" REQUIRED="true" SHOW_IF="USE_BUSINESS_TIMEPERIOD == 'true'">
			<DEFAULT>"'9999-12-30'"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BUSINESS_TIMEPERIOD_END_VALUE_FOR_UPDATE" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="68" REQUIRED="true" SHOW_IF="USE_BUSINESS_TIMEPERIOD == 'true'">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SYSTEM_TIMEPERIOD_START" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="70" REQUIRED="true">
			<DEFAULT>"SYS_VALID_FROM"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SYSTEM_TIMEPERIOD_END" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="70" REQUIRED="true">
			<DEFAULT>"SYS_VALID_TO"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ADDITIONAL_TARGET_COLUMNS" FIELD="TABLE"
			GROUP="TARGET_CONFIG" NUM_ROW="73" NB_LINES="10">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT" VALUE="" />
				<ITEM NAME="TYPE" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="INT">
						<ITEM NAME="SMALLINT" VALUE="SMALLINT" />
						<ITEM NAME="INT" VALUE="INT" />
						<ITEM NAME="BIGINT" VALUE="BIGINT" />
						<ITEM NAME="DECIMAL" VALUE="DECIMAL" />
						<ITEM NAME="DATE" VALUE="DATE" />
						<ITEM NAME="TIMESTAMP" VALUE="TIMESTAMP" />
						<ITEM NAME="VARCHAR" VALUE="VARCHAR" />
					</ITEMS>
				</ITEM>
				<ITEM NAME="LENGTH" FIELD="TEXT" VALUE="10" />
				<ITEM NAME="PRECISION" FIELD="TEXT" VALUE="0" />
				<ITEM NAME="NULLABLE" FIELD="CHECK" VALUE="true" />
				<ITEM NAME="FOR_INSERT" FIELD="CHECK" VALUE="true" />
				<ITEM NAME="FOR_UPDATE" FIELD="CHECK" VALUE="true" />
				<ITEM NAME="VALUE" FIELD="TEXT" VALUE="" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="DELETE_CONDITION" FIELD="MEMO" GROUP="TARGET_CONFIG"
			NUM_ROW="75" >
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DELETE_CONDITION_HINT" FIELD="LABEL" COLOR="0;0;0" NUM_ROW="77" GROUP="TARGET_CONFIG">
			<DEFAULT>The source table has the alias s and the target table has the alias t</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="HIST_TABLE_APPEND_ON" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="79">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SWITCH_ON_VERSIONING_AT_START" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="80">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SWITCH_OFF_VERSIONING_AT_THE_END" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="80" SHOW_IF="SWITCH_ON_VERSIONING_AT_START == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="NO_EXEC_MODE" FIELD="TEXT" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEBUG" FIELD="CHECK" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_STATS" FIELD="TEXT" NUM_ROW="30">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_STATS_FILE" FIELD="FILE" NUM_ROW="40">
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TRANS_START_COLUMN_NAME" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT></DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cimt.talendcomp.db2scdelt" MODULE="cimt.talendcomp.db2scdelt-2.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="Driver-DB2" MODULE="db2jcc.jar"
				UrlPath="platform:/plugin/org.talend.libraries.jdbc.db2/lib/db2jcc.jar"
				REQUIRED="true" />
			<IMPORT NAME="Driver-DB2" MODULE="db2jcc_license_cu.jar"
				UrlPath="platform:/plugin/org.talend.libraries.jdbc.db2/lib/db2jcc_license_cu.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_MERGED" TYPE="id_Integer" />
	</RETURNS>
	
</COMPONENT>